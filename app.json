{
  "expo": {
    "name": "SpotSaver",
    "slug": "spotsaver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#001A33"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spotsaver.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "SpotSaver needs your location to mark where you parked so you can find your way back.",
        "NSCameraUsageDescription": "SpotSaver needs camera access to let you take a photo of your parking spot.",
        "NSPhotoLibraryUsageDescription": "SpotSaver needs photo library access to save parking photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.spotsaver.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SpotSaver to use your location."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SpotSaver to access your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6028e438-ea55-4c95-9aa4-f41f8fea8def"
      }
    }
  }
}